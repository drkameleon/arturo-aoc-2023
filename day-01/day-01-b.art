print sum map read.lines relative "input.txt" 'line [
    converted: translate line #[
        twone:     "21"  oneight:  "18"  eightwo:  "82"  eighthree: "83"
        threeight: "38"  fiveight: "58"  nineight: "98"  sevenine:  "79"
        
        one: "1"  two:   "2"  three: "3"  four: "4"  five: "5" 
        six: "6"  seven: "7"  eight: "8"  nine: "9"
    ]
    numbers: match converted {/\d/}
    to :integer ~"|first numbers||last numbers|"
]